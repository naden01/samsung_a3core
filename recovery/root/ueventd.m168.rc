/dev/usb_accessory        0660   root       usb
/dev/tun                  0660   system     vpn

# CDMA radio interface MUX
/dev/ppp                  0660   radio      vpn

/dev/kvm                  0600   system     system
/dev/vhost-vsock          0600   system	    system

